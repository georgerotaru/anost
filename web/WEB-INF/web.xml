<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>FbEventsMain</servlet-name>
        <servlet-class>ro.anost.servlets.fbevents.FbEventsMain</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FbLogin</servlet-name>
        <servlet-class>ro.anost.servlets.fbevents.FbLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FbGsearch</servlet-name>
        <servlet-class>ro.anost.servlets.fbevents.FbGsearch</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FbLogin</servlet-name>
        <url-pattern>/FbLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FbEventsMain</servlet-name>
        <url-pattern>/FbEventsMain</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FbGsearch</servlet-name>
        <url-pattern>/FbGsearch</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
